<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                               http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Juanda\Entity\Usuario"  table="usuarios">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="username" type="string" nullable="true" />
        <field name="password" type="string" nullable="true" />                      
        <field name="fechaNacimiento" type="date" nullable="true" />
        <field name="bloqueado" type="boolean" nullable="true" />   
                   
        <one-to-many field="etiquetas" target-entity="Juanda\Entity\Etiqueta" mapped-by="usuario" />
               
    </entity>
</doctrine-mapping>
